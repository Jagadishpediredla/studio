
{
  "rules": {
    "desktops": {
      ".read": true,
      ".write": false
    },
    "requests": {
      ".read": true,
      ".write": true
    },
    "status": {
      ".read": true,
      ".write": true
    },
    "logs": {
      ".read": true,
      ".write": true,
      ".indexOn": "createdAt"
    },
    "builds": {
      ".read": true,
      ".write": true
    },
    "binaries": {
      ".read": true,
      ".write": true
    },
    "health_check": {
      "$uid": {
        ".write": true,
        ".read": true,
        ".validate": "newData.hasChildren(['timestamp', 'client']) && newData.getPriority() === null"
      }
    }
  }
}
